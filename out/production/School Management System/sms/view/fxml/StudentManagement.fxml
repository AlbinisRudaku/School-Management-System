      </ImageView>
      <JFXButton fx:id="transfer" buttonType="RAISED" layoutX="711.0" layoutY="304.0" onAction="#transfer" prefHeight="38.0" prefWidth="150.0" stylesheets="@css/RegStudent.css" text="Transfer Students" textFill="WHITE">
