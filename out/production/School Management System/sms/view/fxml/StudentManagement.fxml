                <Image url="@../../other/img/ManageStudent.png" />
            </image>
        </ImageView>
        <ImageView fx:id="StudentMgmt" fitHeight="150.0" fitWidth="200.0" layoutX="40.0" layoutY="150.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../other/img/RegStudent.png" />
            </image>
        </ImageView>
        <JFXButton fx:id="btnStudentReg" buttonType="RAISED" layoutX="36.0" layoutY="304.0" onAction="#SetBtnStudentReg" prefHeight="38.0" prefWidth="150.0" stylesheets="@css/RegStudent.css" text="Student Registration" textFill="WHITE">
            <font>
                <Font name="Calibri Bold" size="14.0" />
            </font>
        </JFXButton>
        <JFXButton fx:id="btnStudentMnge" buttonType="RAISED" layoutX="260.0" layoutY="304.0" onAction="#setBtnStudentMnge" prefHeight="38.0" prefWidth="150.0" stylesheets="@css/RegStudent.css" text="Manage Students" textFill="WHITE">
